{
  "name": "react-docs-markdown",
  "version": "0.1.4",
  "description": "Convert the output of react-docgen to Markdown.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "jest -- --watch",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebuilder/react-docs-markdown.git"
  },
  "keywords": [
    "boilerplate",
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "dsc@insilico.dk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebuilder/react-docs-markdown/issues"
  },
  "homepage": "https://github.com/thebuilder/react-docs-markdown#readme",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "15.0.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.16.0",
    "babel-register": "6.16.3",
    "cross-env": "3.0.0",
    "eslint": "3.6.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "jest": "15.1.1",
    "rimraf": "2.5.4"
  },
  "dependencies": {
    "lodash": "4.16.2",
    "markdown-table": "1.0.0"
  }
}
